#!/usr/bin/env bash

releases_path=https://api.github.com/repos/LogtalkDotOrg/logtalk3/tags
cmd="curl -s $releases_path"

versions=$(eval $cmd | grep -oE "name\": \"lgt.{1,4}stable\"," | sed 's/name\": \"lgt//;s/\",//' | sed 's/stable//;s/\",//' | LC_ALL=C sort | awk '{print $1}' | tr '\n' ' ')
echo $versions
